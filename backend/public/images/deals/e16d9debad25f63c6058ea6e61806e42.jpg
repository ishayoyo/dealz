$(document).ready(function(e) {	
	$('.productsliderajax').each(function () {    	
	var campaignid = $(this).data('camp');
	var offertype = $(this).data('offertype');
	if (typeof campaignid == 'undefined') campaignid="0";
	if (typeof offertype == 'undefined') offertype="0";
    $(this).owlCarousel({
        navigation: true, // Show next and prev buttons
		pagination: false, // Show next and prev buttons
        items: 6,
		itemsDesktop: [1600, 5],
		itemsDesktopSmall: [1024, 4],
        itemsTablet: [768, 3],
        itemsMobile: [500, 2],
		scrollPerPage: true,
		lazyLoad:true, 
        afterUpdate: customPager,
		jsonPath : "LoadindexOffers.asp?campaignid="+campaignid+"&offertype="+offertype,
		afterInit: function(elem)
		{
			customPager(elem);
			observer.observe();	 // Lazy Loading
			$('.timerdeal').each(function () {
				
				var dealStart = new Date($(this).data('dealstart'));
				var dealEnd = new Date($(this).data('dealend'));
				var curDate = new Date();
				var DiffStart 	= curDate - dealStart;
				var DiffEnd 	= curDate - dealEnd;
				var liftoff,layout,liftoffmode;
				if (DiffStart>=0 && DiffEnd<=0)
				{
					liftoff = dealEnd;
					layout = '{dnn}:{hnn}:{mnn}:{snn}';
					liftoffmode=1;
				}else{
					liftoff = dealStart;
					layout = '{mn}:{snn}';
					liftoffmode=0;
				}
					$(this).countdown({until: liftoff,  layout: layout
					 ,onExpiry: function endcount() {$(this).html('המכירה הסתיימה')}
					});
			});		
		}
    });
	});


});
